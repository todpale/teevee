{
  "name": "teevee",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --watch",
    "test:coverage": "vitest --ui --coverage",
    "test:coverage:raw": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "release": "changelogen --release --push",
    "icons:export": "node scripts/icons-export.js",
    "icons:convert": "node scripts/icons-to-json.js",
    "icons:generate": "pnpm icons:export && pnpm icons:convert"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.72",
    "@iconify/tools": "^4.1.4",
    "@iconify/utils": "^3.0.2",
    "@iconify/vue": "^5.0.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-istanbul": "^4.0.6",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vitest/ui": "4.0.6",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "changelogen": "^0.6.2",
    "eslint": "^9.37.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-vue": "~10.5.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^4.0.6",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n": "^11.1.12",
    "vue-tsc": "^3.1.1"
  }
}
